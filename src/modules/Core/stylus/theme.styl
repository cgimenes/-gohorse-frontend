/** Stylus Styles */
@import '../../../../node_modules/vuetify/src/stylus/settings/_colors'

$theme := {
  primary: $grey.darken-4
  accent: $red.accent-2
  secondary: $grey.lighten-2
  info: $blue.base
  warning: $amber.base
  error: $red.base
  success: $purple.base
}

$gradients := {
  primary: linear-gradient(to right, #f857a6, #ff5858)
  pink: linear-gradient(to right, #f857a6, #ff5858)
  teal: linear-gradient(to right, #43cea2, #185a9d)
  red: linear-gradient(to right, #cb2d3e, #ef473a)
  blue: linear-gradient(to right, #00d2ff, #3a7bd5)
  orange: linear-gradient(to right, #FF4E50, #F9D423)
  yellow-green: linear-gradient(to right, #dce35b, #45b649)
  green: linear-gradient(to right, #76b852, #8dc26f)
  green-teal: linear-gradient(to right, #348f50, #56b4d3)
  teal-blue: linear-gradient(to right, #43cea2, #185a9d)
  tdasdasdsaue: linear-gradient(to right, #43cea2, #185a9d)
}
